{
  "name": "light-engine",
  "version": "2.0.4",
  "description": "Light Engine is a game engine by Grozzlo",
  "main": "light.js",
  "type": "module",
  "types": "types/index.d.ts",
  "module": "lib/app.js",
  "files": [
    "globals.d.ts",
    "index.d.ts",
    "app.js",
    "EventEmitter.js",
    "FpsController.js",
    "helper.js",
    "animations/**/*",
    "core/**/*",
    "gameobjects/**/*",
    "hooks/**/*",
    "loaders/**/*",
    "entities/**/*",
    "managers/**/*",
    "nodes/**/*",
    "states/**/*",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./app.js"
    },
    "./animations": {
      "import": "./animations/index.js"
    },
    "./core": {
      "import": "./core/index.js"
    },
    "./entities": {
      "import": "./entities/index.js"
    },
    "./gameobjects": {
      "import": "./gameobjects/index.js"
    },
    "./hooks": {
      "import": "./hooks/index.js"
    },
    "./loaders": {
      "import": "./loaders/index.js"
    },
    "./managers": {
      "import": "./managers/index.js"
    },
    "./nodes": {
      "import": "./nodes/index.js"
    },
    "./states": {
      "import": "./states/index.js"
    }
  },
  "repository": "https://github.com/Grozzlo/light-engine",
  "author": "ashikuri",
  "license": "CC0-1.0",
  "scripts": {
    "build": "tsc && node ./copy.js",
    "dev": "tsc --watch",
    "prepack": "npm run build"
  },
  "devDependencies": {
    "@types/localforage": "^0.0.34",
    "@types/node": "^17.0.14",
    "@types/sat": "^0.0.31",
    "prettier": "^2.5.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "localforage": "^1.9.0",
    "sat": "^0.9.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}